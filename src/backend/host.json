{
    "version": "2.0",
    "logging": {
        "logLevel": {
            // "default": "Information"
            "backend.Healthz": "Information"
        },
        "extensions": {
            "durableTask": {
                "hubName": "ToDoApp"
            }
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    }
}